#!/bin/bash
#
DWORD=$(cat $(pwd)/d_word)
#A=""
#
func_one ()
{
	if [[ -z "$A" ]]
	then
		for word in $DWORD
		do
			echo $word; func_two
		done
	else
		for word in $DWORD
		do
		echo $A$word
	done
	fi

}
#
func_two () 
{
	for A in $DWORD
	do
		echo "[][$A][]"; func_one
	done
}
func_one

